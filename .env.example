# Moneybird API Token (required)
# Generate one at https://moneybird.com/user/applications
MONEYBIRD_API_TOKEN=your_api_token_here

# Moneybird Administration ID (required)
# Can be found in the URL when logged in to Moneybird: https://moneybird.com/[YOUR_ADMINISTRATION_ID]/
MONEYBIRD_ADMINISTRATION_ID=your_administration_id_here

# MCP Server Port (required)
MCP_SERVER_PORT=3000

# Contact Email (optional)
CONTACT_EMAIL=your_email@example.com 